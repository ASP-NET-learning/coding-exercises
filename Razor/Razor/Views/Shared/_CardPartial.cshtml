@model Razor.Models.Card

<div class="card" style="width: 18rem;">
    <img src="~/www/image/@Model.Photo" class="card-img-top" alt="">
    <div class="card-body">
        <h5 class="card-title">@Model.Name</h5>
        <p class="card-text">@Model.Brief</p>
        <a href="@Model.WikiUrl" class="btn btn-primary">Wiki</a>
    </div>
</div>